{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/*"
      ],
      "@/*": [
        "./*"
      ],
    },
    "jsx": "preserve",
    "strict": false,
    "target": "ES2015",
    "module": "ESNext",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "lib": [
      "esnext",
      "dom"
    ]
  }
}